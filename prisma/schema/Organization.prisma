model Organization {
  id          String      @id @default(nanoid(4))
  name        String      @unique
  slug        String      @unique
  email       String?
  phone       String?
  address     String?
  logoUrl     String?
  isActive    Boolean     @default(true)
  createdAt   DateTime    @default(now())
  updatedAt   DateTime    @updatedAt
  
  users       User[]
  timetables  Timetable[]
}