model LectureSlot {
  id        String  @id @default(nanoid(4))
  slotId    String
  lectureId String
  isLocked  Boolean @default(false)
  slot      Slot    @relation(fields: [slotId], references: [id])
  lecture   Lecture @relation(fields: [lectureId], references: [id])

  @@unique([slotId, lectureId])
}
